\name{modelDisplay}
\Rdversion{1.0}
\alias{modelDisplay}
\alias{gpdisimDisplay}
\alias{gpsimDisplay}
\alias{cmpndKernDisplay}
\alias{disimKernDisplay}
\alias{kernDisplay}
%\alias{mlpKernDisplay}
