\name{rbfKernParamInit}
\Rdversion{1.0}
\alias{rbfKernParamInit}
\title{RBF kernel parameter initialisation.}
\description{
  initialises the radial basis function kernel structure with some default parameters.
}
\usage{
rbfKernParamInit(kern)
}
\arguments{
  \item{kern}{the kernel structure which requires initialisation.}
}
\value{
  \item{kern}{the kernel structure with the default parameters placed in.}
}
\seealso{
\code{\link{kernCreate}, \link{kernParamInit}}.
}
\examples{
## missing
}
\keyword{model}
