\name{multiKernParamInit}
\Rdversion{1.0}
\alias{multiKernParamInit}
\title{MULTI kernel parameter initialisation.}
\description{
  initialises the multiple output block kernel structure with some default parameters.
}
\usage{
multiKernParamInit(kern)
}
\arguments{
  \item{kern}{the kernel structure which requires initialisation.}
}
\value{
  \item{kern}{the kernel structure with the default parameters placed in.}
}
\seealso{
\code{
  \link{kernCreate}, \link{kernParamInit}}.
}
\examples{
## missing
}
\keyword{model}
